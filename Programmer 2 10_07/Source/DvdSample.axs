(***********************************************************)
(*                STARTUP CODE GOES BELOW                  *)
(***********************************************************)
DEFINE_START

(***********************************************************)
(*                THE EVENTS GO BELOW                      *)
(***********************************************************)
DEFINE_EVENT

button_event[dvTp,nBtns_DvdMenu]
    {
    push:
	{
	stack_var integer nIndex
	nIndex=get_last(nBtns_DvdMenu)
	
	if(nDiscType=nDvd)
	    {
	    send_string dvDvd, "chrCmds_DvdMenu[nIndex]"
	    }
	
	}
    }

