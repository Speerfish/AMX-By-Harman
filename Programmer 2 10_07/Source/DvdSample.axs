(***********************************************************)
DEFINE_TYPE

(***********************************************************)
(*               VARIABLE DEFINITIONS GO BELOW             *)
(***********************************************************)
DEFINE_VARIABLE

INTEGER nDiscType = nDvd

INTEGER nBtns_DvdMenu[]=
    {
    1,2,3,4,5,6
    }

char chrCmds_DvdMenu[]=
    {
    {$24,$2A,$0D},
    {$24,$2B,$0D},
    {$24,$2C,$0D},
    {$24,$2D,$0D},
    {$24,$2E,$0D},
    {$24,$2F,$0D}
    }




